assert = fun(expr, msg) -> {
	if (!expr) {
		print("Assertion failed", msg)
		exit(-1)
	}
}

bexp = 1 + 2 + 3 + 4
assert(bexp == 10, "Binary expression")
X = fun(x0, v0, a0, t) = x0 + v0*t + 1/2 * a0 * t^2
res = X(1, 3.5, 10, 3)
assert( res > 56 && res < 57, "Complex expression, result: " + res)
print(res)

